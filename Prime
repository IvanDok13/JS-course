  function isPrime(n){
      for (let i = 2, max = Math.sqrt(n); i <= max; i++) {
        if (n % i === 0) return false;
      }
      return n > 1;
  }
     console.log(isPrime(7));

function numbPrime(n) {
  let arr = [];
  for (let i = 2; i <= n; i++) {
    if (isPrime(i) === true) {
      arr.push(i);
    }
  }
  return arr;
}
  
console.log(numbPrime(6));
